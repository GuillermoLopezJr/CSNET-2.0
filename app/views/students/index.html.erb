
<table>
  <thead>
    <tr>
      <th>Course Name</th>
      <th>Student Email</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
 
  <% @students.each do |student| %>
    <tr>
      <% if student.courses != nil %>
         <% student.courses.each do |course| %>
          <td><%= course.name %> <%= course.number %></td>
         <% end %>
          <td><%= student.email %></td>
      <% end %>
          <td></td>
          <td></td>
    </tr>
    
  <% end %>
</table>

<%= link_to 'Home', root_path %>