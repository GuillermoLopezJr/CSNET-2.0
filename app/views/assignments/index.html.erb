<div id="assign-list-content">
  <h1>Listing Assignments</h1>
   
  <table>
    <thead>
      <tr>
        <th>Assignment</th>
        <th>Course</th>
        <th>Due Date</th>
        <th>Actions</th>
      </tr>
    </thead>
  
    <% @assignments.each do |assignment| %>
      <tr>
        <td><%= assignment.name %></td>
        <td><%= assignment.course_num %></td>
        <td><%= assignment.due_date %></td>
        <td style="text-align:center"><%= link_to 'Show', assignment_path(assignment) %> | <%= link_to 'Edit', assignment_path(assignment) %></td>
      </tr>
    <% end %>
</table>
 <p>  <%= link_to 'Home', instructor_path(:index) %> </p>
</div>
